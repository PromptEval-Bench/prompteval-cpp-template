{
  "files.associations": {
    "*.h": "cpp",
    "*.cpp": "cpp",
    "*.hpp": "cpp"
  },
  "C_Cpp.default.includePath": [
    "${workspaceFolder}/include",
    "${workspaceFolder}/src"
  ],
  "C_Cpp.default.browse.path": [
    "${workspaceFolder}/include",
    "${workspaceFolder}/src"
  ],
  "editor.formatOnSave": true,
  "C_Cpp.clang_format_style": "Google"
}